<Window x:Class="PictureLibaryLink.ChooseList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PictureLibaryLink"
        mc:Ignorable="d"
        Title="ChooseList" Height="173.762" Width="394.554">
    <Grid>
        <Button x:Name="btnChoose" Content="Auswählen" HorizontalAlignment="Left" Margin="32,93,0,0" VerticalAlignment="Top" Width="89" Height="20" Click="btnChoose_Click"/>
        <ComboBox x:Name="cbLists" HorizontalAlignment="Left" Margin="32,51,0,0" VerticalAlignment="Top" Width="321"/>
        <Button x:Name="btnNewList" Content="Neue Liste" HorizontalAlignment="Left" Margin="150,93,0,0" VerticalAlignment="Top" Width="89" Height="20" Click="btnNewList_Click"/>
        <Button x:Name="btnAbort" Content="Abbrechen" HorizontalAlignment="Left" Margin="264,93,0,0" VerticalAlignment="Top" Width="89" Height="20" Click="btnAbort_Click"/>
        <Label x:Name="lbTitle" Content="Liste auswählen" HorizontalAlignment="Left" Margin="150,20,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
